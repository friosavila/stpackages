*! v0.1 alt to tabstat
program drop tabstat2
program tabstat2, rclass
	syntax anything(everything) [aw iw fw] [if] [in], [save *]
	tabstat `0'
	tempname smatrix
	local j=1
	if "`save'"!="" {		
		while _rc==0 {
			matrix `smatrix' = nullmat(`smatrix')\r(Stat`j')
			local j=`j'+1
			capture confirm matrix r(Stat`j')
		}
	}
	return add
	return matrix tmatrix = `smatrix'
end